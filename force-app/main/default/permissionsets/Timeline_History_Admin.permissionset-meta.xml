<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Provides access to modify Timeline History Component configuration and use the
        History Report tab. Assign to admins who need to add or remove child objects from timeline
        views and run cross-object history reports.</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Timeline History Admin</label>
    <customPermissions>
        <enabled>true</enabled>
        <name>Modify_Timeline_Config</name>
    </customPermissions>
    <classAccesses>
        <apexClass>TimelineHistoryController</apexClass>
        <enabled>true</enabled>
    </classAccesses>
    <classAccesses>
        <apexClass>TimelineConfigController</apexClass>
        <enabled>true</enabled>
    </classAccesses>
    <classAccesses>
        <apexClass>HistoryReportController</apexClass>
        <enabled>true</enabled>
    </classAccesses>
</PermissionSet>